package mytest;


public class Bean_Point {


	public int x;
	public int y;
		
	public Bean_Point(){
		this.x=0;
		this.y=0;
	}
	public Bean_Point(int x, int y) {
		this.x = x;
		this.y = y;
	}
	public int getX() {
		return x;
	}
	public void setX(int x) {
		this.x = x;
		}
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y=y;
	}
	
	public static void main(String[] args) {
		
		Bean_Point p = new Bean_Point();
	
		

	}
}

	
		



	
	
	
		





	
	
	
		

