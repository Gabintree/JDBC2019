    pageEncoding="UTF-8"%>
<%
	String redirect = request.getParameter("redirect");

	if(redirect.equals("redirect")) {
		response.sendRedirect("http://localhost:8181/Jsp0516_PageMovingRedirect.jsp");
	}else if(redirect.equals("forward")){
		response.sendRedirect("http://localhost:8181/Jsp0516_PageMovingForward.jsp");
	}

%>    
    
    
